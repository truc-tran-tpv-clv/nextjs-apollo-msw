{
  "name": "ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "yarn test:clear && DEBUG_PRINT_LIMIT=900000 jest --watchAll",
    "test:ci": "jest --ci",
    "test:clear": "jest --clearCache",
    "test:cov": "yarn test:clear && jest --coverage",
    "lint:format": "prettier --write \"./**/*.(ts|js|tsx|json)\" ",
    "eslint:fix": "eslint . --ext .ts,.tsx,.json --fix",
    "lint": "yarn lint:format && next lint --no-cache --fix",
    "codegen": "DOTENV_CONFIG_PATH=./.env.test graphql-codegen --config codegen.ts -r dotenv/config"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@apollo/client": "^3.8.0-beta.2",
    "@apollo/experimental-nextjs-app-support": "^0.2.1",
    "antd": "^5.6.1",
    "cross-fetch": "^3.1.6",
    "dayjs": "^1.11.8",
    "framer-motion": "^10.12.16",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "next": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@graphql-codegen/cli": "4.0.1",
